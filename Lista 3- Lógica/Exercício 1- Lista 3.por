programa
{
	
	funcao inicio()
	{    inteiro  numero, i=0, soma=0
	
		escreva("Digite um numero: ")
		leia(numero)
		faca{
			  i++
			  soma = soma + i
			  escreva(i, "\n")
		}
		  enquanto(i<numero)
		     escreva(soma)
	}
}
